{
    "configurations": [
        {
            "name": "VERY VERY GANG",
            "type": "cppdbg",
            "request": "launch",
            "command": "/usr/bin/gcc"
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}.out",
                "$(${workspaceFolder}/pkg-config.sh --libs --cflags allegro-5 allegro_font-5 allegro_image-5 allegro_ttf-5 allegro_primitives-5)"
              ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ],
    "version": "2.0.0"
}